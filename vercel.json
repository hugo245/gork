{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/gork/(.*)",
      "dest": "/gork/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "rewrites": [
    {
      "source": "/gork/(.*)",
      "destination": "/"
    },
    {
      "source": "/(.*)",
      "destination": "/gork/$1"
    }
  ]
}