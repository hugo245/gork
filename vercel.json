{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/gork/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/gork/(.*\\.(html|js|css|json))",
      "dest": "/$1"
    },
    {
      "src": "/gork/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/gork",
      "dest": "/index.html"
    },
    {
      "src": "/",
      "dest": "/gork"
    }
  ]
}